{
  "root": true,
  "parserOptions": {
    "parser": "babel-eslint"
  },
  "plugins": [
    "html",
    "vue"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:vue/recommended"
  ],
  "rules": {
    "no-console": "off"
  },
  "env": {
    "browser": true,
    "node": true,
    "commonjs": true,
    "es6": true
  }
}